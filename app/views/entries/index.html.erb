<ul>
<% @project.entries.each do |e|%>
	<li><%= @project.name %>
		<ul>
		<li><%= e.hours %> hours</li>
		<li><%= e.minutes %> minutes</li>
		<li><%= e.date %></li>
		</ul>
	</li>
	<%= link_to('Delete', project_entry_path(@project, e), :method => :delete, :confirm =>'Are you sure you want to delete?') %>
<% end %>
</ul>

<button type="submit" action="/projects/:project_id/entries/new">Create Entry</button>